.container
    ul#menu
        li
            a.btn(href="#list")
                i.icon-th-list.icon-white
                = t('List')
            a.btn(href="#calendar")
                i.icon-calendar.icon-white
                = t('Calendar')
            a.btn(href="export/calendar.ics", target="_blank")
                i.icon-share.icon-white
                = t('Export')
            a.active.btn(href="#import")
                i.icon-circle-arrow-up.icon-white
                = t('Import')

    #import-form.well
        h3= t('ICalendar importer')

        div.import-form
            button#import-button.btn= t('import your icalendar file')
            input#import-file-input(type="file")

        div.confirmation
            button#confirm-import-button.btn= t('confirm import')
            button#cancel-import-button.btn=t ('cancel')

        div.results
            h4= t('Alarms to import')
            #import-alarm-list

            h4= t('Events to import')
            #import-event-list
